{
    "include": [
        "**/*",
        "**/.server/**/*",
        "**/.client/**/*",
        ".react-router/types/**/*",
        "./../../../packages/components"
    ],
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2022"
        ],
        "types": [
            "vite/client"
        ],
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "rootDirs": [
            ".",
            "./.react-router/types"
        ],
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./app/*"
            ],
            "@/*": [
                "./*"
            ],
            "@app/components/*": [
                "./../../../packages/components/src/*"
            ],
            "@wails/*": [
                "./wailsjs/go/main/*"
            ]
        },
        "allowJs": true,
        "esModuleInterop": true,
        "verbatimModuleSyntax": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true
    },
    "references": [
        {
            "path": "./../../../packages/components"
        }
    ]
}